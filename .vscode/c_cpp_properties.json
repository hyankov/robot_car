{
    "configurations": [
        {
            "name": "Win32",
            "defines": [
                "__AVR__",
                "__AVR_ATmega328P__",
                "ARDUINO=10800"
            ],
            "includePath": [
                "${workspaceRoot}",
                "${env:programfiles(x86)}\\Arduino\\hardware\\arduino\\avr\\**",
                "${env:programfiles(x86)}\\Arduino\\tools\\**",
                "${env:programfiles(x86)}\\Arduino\\hardware\\tools\\**",
                "${env:programfiles(x86)}\\Arduino\\libraries\\**",
                "${env:userprofile}\\Documents\\Arduino\\hardware\\tools\\**",
                "${env:userprofile}\\Documents\\Arduino\\libraries\\**",
                "${env:userprofile}\\Documents\\Arduino\\libraries\\NewPing\\src",
                "${env:userprofile}\\Documents\\Arduino\\libraries\\U8glib\\src"
            ],
            "forcedInclude": [
                "${env:programfiles(x86)}\\Arduino\\hardware\\arduino\\avr\\cores\\arduino\\Arduino.h"
            ],
            "intelliSenseMode": "${default}",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "compilerPath": "${env:programfiles(x86)}/Microsoft Visual Studio/2017/Professional/VC/Tools/MSVC/14.16.27023/bin/Hostx64/x64/cl.exe"
        }
    ],
    "version": 4
}